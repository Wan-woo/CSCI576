   "source": [
    "import cv2\n",
    " \n",
    "capture = cv2.VideoCapture('video1.mp4')\n",
    " \n",
    "frameNr = 0\n",
    " \n",
    "while (frameNr < 1000):\n",
    " \n",
    "    success, frame = capture.read()\n",
    " \n",
    "    if success and frameNr % 10 == 0:\n",
    "        cv2.imwrite('images/frame_'+str(frameNr)+'.jpg', frame)\n",
    " \n",
    "    frameNr = frameNr+1\n",
    " \n",
    "capture.release()"
   ]
  }
